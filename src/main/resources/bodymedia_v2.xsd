  <xs:complexType name="record">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[A record object represents best performance of its kind that has been
previously measured or noted. There are currently ten types of records:
EXPENDITURE, MOD_ACTIVITY, VIG_ACTIVITY, ACTIVITY, METS, STEPS, SLEEP,
LYING_DOWN, SLEEP_EFFICIENCY, and MINUTES_ONBODY.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:attribute name="rangeType" type="rangeType">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The length of time that is record is measured. Currently this can only be DAY.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="value" use="required" type="xs:double">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The value of this record.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="date" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The date this record was set.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="recordType" type="recordType">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[The type of this record currently one of: EXPENDITURE, MOD_ACTIVITY, VIG_ACTIVITY, ACTIVITY, METS, STEPS, SLEEP, LYING_DOWN, SLEEP_EFFICIENCY, or MINUTES_ONBODY.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="note" type="xs:string">
      <xs:annotation>
        <xs:documentation>
          <![CDATA[A note for this record.]]>
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="serviceError">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Represents an error returned from the service.]]>
