                <xs:documentation>
                  <![CDATA[A list of detail error codes and descriptions that caused this error.]]>
                </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
