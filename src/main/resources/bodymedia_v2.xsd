      <xs:element name="dayStart" type="timeDTO" minOccurs="0"/>
      <xs:element name="dayEnd" type="timeDTO" minOccurs="0"/>
      <xs:element name="requestedTime" type="timeDTO" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="timezoneDTO">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Represents time zones that have been set by the user. Each time a user updates their time zone
a new time zone entry is created.
The time zone values are chained together in time via their associated start and end
dates, with the start date of the subsequent equaling the end date of the
previous time zone. A time zone without an endDate is considered the
current time zone.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="startDate" type="xs:string"/>
        <xs:attribute name="endDate" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="timezoneRangeDTO">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[Represents time zones that have been set by the user. Each time a user updates their time zone
a new time zone entry is created.
The time zone values are chained together in time via their associated start and end
dates, with the start date of the subsequent equaling the end date of the
previous time zone. A time zone without an endDate is considered the
current time zone.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="timezone" type="timezoneDTO" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[A list of timezones set by the user.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="userInfoDTO">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[This object holds basic user information.]]>
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[A unique id for this user.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="externalId" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[A custom user identifier for the application using the BodyMedia FIT API.
If this ID not been set by the application, it will not be present.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="birthday" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The users birth date.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="timezone" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[The user's current time zone setting.]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="weight" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>
